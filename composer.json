{
  "name": "webrtb/address-splitter",
  "description": "Split street, house number and addition from address.",
  "keywords": [
    "split",
    "address",
    "street",
    "housenumber",
    "addition"
  ],
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "*",
    "symfony/string": "^5.1 || ^6.0 || ^7.0",
    "ext-json": "*"
  },
  "require-dev": {
    "fzaninotto/faker": "1.9.1",
    "symfony/var-dumper": "^5.1",
    "phpunit/phpunit": "9.2.2"
  },
  "autoload": {
    "psr-4": {
      "WebRTB\\AddressSplitter\\App\\": "src/app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebRTB\\AddressSplitter\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
